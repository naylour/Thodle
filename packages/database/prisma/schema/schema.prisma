generator client {
    provider = "prisma-client-js"
}

generator markdown {
  provider = "prisma-markdown"
  output   = "./ERD.md"
  title    = "Thodle ERD"
}

datasource db {
    provider = "postgresql"
    url      = env("POSTGRE_URL")
    schemas  = ["default", "user", "university", "study", "universal"]
}
